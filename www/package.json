{
  "name": "httptest-www",
  "version": "0.0.0",
  "description": "",
  "author": "Jan Svager <jan@svager.cz>",
  "license": "MIT",
  "homepage": "https://github.com/svagi/httptest#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/svagi/httptest.git"
  },
  "bugs": {
    "url": "https://github.com/svagi/httptest/issues"
  },
  "keywords": [
    "http",
    "http/2",
    "HAR",
    "performance"
  ],
  "main": "src/index.js",
  "babel": {
    "presets": ["es2015", "react"],
    "plugins": ["transform-object-rest-spread"]
  },
  "scripts": {
    "babel:build": "babel src -d build",
    "nodemon:watch": "nodemon -L --watch src/ --exec babel-node src/server.js",
    "webpack:build": "webpack --config src/webpack.config.js --colors",
    "webpack:watch": "npm run webpack:build -- --watch",
    "start": "npm run babel:build && npm run webpack:build && node build/server.js",
    "watch": "npm run nodemon:watch & npm run webpack:watch"
  },
  "dependencies": {
    "babel-cli": "6.7.5",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-object-rest-spread": "6.6.5",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "bluebird": "^3.3.4",
    "body-parser": "1.15.0",
    "css-loader": "0.23.1",
    "express": "4.13.4",
    "morgan": "1.7.0",
    "postcss-loader": "0.8.2",
    "react-dom": "15.0.1",
    "react-router": "2.0.1",
    "react": "15.0.1",
    "style-loader": "0.13.1",
    "webpack": "1.13.0",
    "valid-url": "1.0.9"
  },
  "devDependencies": {
    "nodemon": "^1.9.1"
  }
}
