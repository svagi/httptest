{
  "name": "httptest-api",
  "version": "0.1.0",
  "description": "",
  "author": "Jan Svager <jan@svager.cz>",
  "license": "MIT",
  "homepage": "https://github.com/svagi/httptest#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/svagi/httptest.git"
  },
  "bugs": {
    "url": "https://github.com/svagi/httptest/issues"
  },
  "keywords": [
    "http",
    "http/2",
    "HAR",
    "performance"
  ],
  "main": "src/index.js",
  "babel": {
    "presets": ["es2015"],
    "plugins": ["transform-object-rest-spread"]
  },
  "ava": {
    "babel": "inherit"
  },
  "scripts": {
    "build": "babel src -d build",
    "lint": "standard --verbose src/*",
    "start": "npm run build && node build/index.js",
    "test": "npm run lint && ava --require 'babel-register' -v src/**/*.test.js",
    "watch": "nodemon -L --watch src/ --exec babel-node src/index.js"
  },
  "dependencies": {
    "babel-cli": "^6.7.7",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "bluebird": "^3.3.5",
    "body-parser": "^1.15.0",
    "chokidar": "^1.4.3",
    "express-validator": "^2.20.3",
    "express": "^4.13.4",
    "morgan": "^1.7.0",
    "node-uuid": "^1.4.7",
    "selenium-webdriver": "^2.53.2"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "nodemon": "1.9.2",
    "standard": "^7.0.0",
    "supertest": "^1.2.0",
    "ava": "^0.14.0"
  }
}
