{
  "name": "httptest",
  "version": "0.0.0",
  "description": "",
  "author": "Jan Svager <jan@svager.cz>",
  "license": "MIT",
  "homepage": "https://github.com/svagi/httptest#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/svagi/httptest.git"
  },
  "keywords": ["HTTP/2"],
  "bugs": {
    "url": "https://github.com/svagi/httptest/issues"
  },
  "main": "src/index.js",
  "standard": {
    "parser": "babel-eslint"
  },
  "babel": {
    "presets": ["es2015"],
    "plugins": ["transform-object-rest-spread"]
  },
  "scripts": {
    "build": "babel src -d build",
    "lint": "standard --verbose src/*",
    "start": "NODE_ENV=production npm run build && node build/index.js",
    "test": "npm run lint && ava --require 'babel-register' -v src/**/*.test.js",
    "watch": "nodemon -L --ignore node_modules/ --exec babel-node src/index.js"
  },
  "dependencies": {
    "babel-cli": "^6.5.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-plugin-transform-object-rest-spread": "^6.6.4",
    "bluebird": "^3.3.3",
    "body-parser": "^1.15.0",
    "express": "^4.13.4",
    "express-validator": "^2.19.1",
    "morgan": "^1.7.0",
    "url-parse": "^1.0.5"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0",
    "nodemon": "1.9.0",
    "standard": "^6.0.7",
    "supertest": "^1.2.0",
    "ava": "^0.12.0"
  }
}
