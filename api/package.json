{
  "name": "httptest",
  "version": "0.0.0",
  "description": "",
  "author": "Jan Svager <jan@svager.cz>",
  "license": "MIT",
  "homepage": "https://github.com/svagi/httptest#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/svagi/httptest.git"
  },
  "bugs": {
    "url": "https://github.com/svagi/httptest/issues"
  },
  "keywords": [
    "http",
    "http/2",
    "HAR",
    "performance"
  ],
  "main": "src/server.js",
  "babel": {
    "presets": ["es2015", "react"],
    "plugins": ["transform-object-rest-spread"]
  },
  "scripts": {
    "babel:build": "babel src -d build",
    "nodemon:watch": "nodemon -L --watch src/ --exec babel-node src/server.js",
    "webpack:build": "webpack --colors",
    "webpack:watch": "npm run webpack:build -- --watch",
    "build": "npm run babel:build && npm run webpack:build",
    "start": "node build/server.js",
    "watch": "npm run nodemon:watch & npm run webpack:watch"
  },
  "dependencies": {
    "babel-cli": "6.18.0",
    "babel-loader": "6.2.7",
    "babel-plugin-transform-object-rest-spread": "6.16.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "bluebird": "3.3.4",
    "compression-webpack-plugin": "0.3.2",
    "css-loader": "0.25.0",
    "debug": "2.3.2",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "chrome-har-capturer": "0.9.0",
    "ioredis": "2.4.0",
    "morgan": "1.7.0",
    "postcss-loader": "1.1.1",
    "pluralize": "3.0.0",
    "react-dom": "15.4.0",
    "react-router": "3.0.0",
    "react": "15.4.0",
    "style-loader": "0.13.1",
    "uuid": "2.0.3",
    "valid-url": "1.0.9",
    "webpack": "1.13.3"
  },
  "devDependencies": {
    "nodemon": "^1.9.1"
  }
}
