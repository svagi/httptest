{
    "name": "httptest",
    "version": "1.0.0-rc1",
    "description": "",
    "author": "Jan Svager <jan@svager.cz>",
    "license": "MIT",
    "homepage": "https://www.httptest.net",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/svagi/httptest.git"
    },
    "bugs": {
        "url": "https://github.com/svagi/httptest/issues"
    },
    "keywords": [
        "http",
        "http/2",
        "HAR",
        "performance"
    ],
    "main": "src/server.js",
    "scripts": {
        "babel:build": "BABEL_ENV=node babel src -d build",
        "nodemon:watch": "BABEL_ENV=node nodemon -L --watch src/ --exec babel-node src/server.js",
        "webpack:build": "BABEL_ENV=webpack webpack --colors",
        "webpack:watch": "npm run webpack:build -- --watch",
        "build": "npm run babel:build && npm run webpack:build",
        "lint": "standard src/*",
        "start": "npm run build && node build/server.js",
        "watch": "npm run nodemon:watch & npm run webpack:watch",
        "test": "BABEL_ENV=node ava"
    },
    "ava": {
        "files": [
            "src/**/*.test.js"
        ],
        "babel": "inherit",
        "require": [
            "babel-register"
        ],
        "verbose": true
    },
    "standard": {
        "parser": "babel-eslint"
    },
    "dependencies": {
        "babel-cli": "6.18.0",
        "babel-loader": "6.2.7",
        "babel-preset-env": "0.0.8",
        "babel-preset-react": "6.16.0",
        "babel-preset-stage-3": "6.17.0",
        "bluebird": "3.3.4",
        "compression-webpack-plugin": "0.3.2",
        "css-loader": "0.25.0",
        "debug": "2.3.2",
        "etag": "1.7.0",
        "express": "4.14.0",
        "extract-text-webpack-plugin": "1.0.1",
        "hiredis": "0.5.0",
        "chrome-har-capturer": "0.9.2",
        "ioredis": "2.4.0",
        "morgan": "1.7.0",
        "pluralize": "3.0.0",
        "postcss-loader": "1.1.1",
        "rc-progress": "2.0.3",
        "react-dom": "15.4.0",
        "react-router": "3.0.0",
        "react": "15.4.0",
        "regenerator-runtime": "0.9.6",
        "style-loader": "0.13.1",
        "uuid": "2.0.3",
        "valid-url": "1.0.9",
        "webpack": "1.13.3"
    },
    "devDependencies": {
        "ava": "*",
        "babel-register": "*",
        "babel-eslint": "*",
        "nodemon": "*",
        "standard": "*"
    }
}
